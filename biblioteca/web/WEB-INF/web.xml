<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>login.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cadastraAssociado</servlet-name>
        <servlet-class>cadastros.cadastraAssociado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperationServlet</servlet-name>
        <servlet-class>controller.OperationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cadastraPublicacao</servlet-name>
        <servlet-class>cadastros.cadastraPublicacao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cadastraExemplar</servlet-name>
        <servlet-class>cadastros.cadastraExemplar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consultaPublicacao</servlet-name>
        <servlet-class>consultas.consultaPublicacao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>realizaEmprestimo</servlet-name>
        <servlet-class>operacoes.realizaEmprestimo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>realizaDevolucao</servlet-name>
        <servlet-class>operacoes.realizaDevolucao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>geraRelatorio</servlet-name>
        <servlet-class>relatorios.geraRelatorio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consultaPublicacaoAssoc</servlet-name>
        <servlet-class>consultas.consultaPublicacaoAssoc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consultaHistorico</servlet-name>
        <servlet-class>consultas.consultaHistorico</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cadastraAssociado</servlet-name>
        <url-pattern>/cadastraAssociado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperationServlet</servlet-name>
        <url-pattern>/OperationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cadastraPublicacao</servlet-name>
        <url-pattern>/cadastraPublicacao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cadastraExemplar</servlet-name>
        <url-pattern>/cadastraExemplar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consultaPublicacao</servlet-name>
        <url-pattern>/consultaPublicacao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>realizaEmprestimo</servlet-name>
        <url-pattern>/realizaEmprestimo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>realizaDevolucao</servlet-name>
        <url-pattern>/realizaDevolucao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>geraRelatorio</servlet-name>
        <url-pattern>/geraRelatorio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consultaPublicacaoAssoc</servlet-name>
        <url-pattern>/consultaPublicacaoAssoc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consultaHistorico</servlet-name>
        <url-pattern>/consultaHistorico</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
